<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" 
     android:paddingTop="10dp"
    android:paddingBottom="10dp"
    >
    
    <TextView 
        android:padding="10dp"
         android:id="@+id/TextView_baoyangmoving_km_number_before001"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/inputblack"
        android:text="@string/text_get_aicar"/>
    <TextView 
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:padding="10dp"
        />
    <LinearLayout 
        android:padding="10dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" 
    android:gravity="center">
    <ImageView 
        android:layout_margin="2dp"
        android:id="@+id/ImageView_aicar_get_before001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/car_mini"
        android:visibility="visible"
        />
    <TextView 
        android:visibility="gone"
        android:id="@+id/TextView_baoyangmoving_price_before001"
        android:gravity="center_vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         android:textColor="@color/inputblack"
        android:text=""/>
    <TextView 
        android:id="@+id/TextView_baoyangmoving_price_to_back_before001"
        android:gravity="center_vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         android:textColor="@color/inputblack"/>
    </LinearLayout>

</LinearLayout>
