<!-- 选择车系信息页面 body部分 -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
	android:background="@color/transparent">
	<LinearLayout
	    android:id="@+id/LinearLayout_get001"
	    android:layout_marginTop="1dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
	android:orientation="horizontal"
	android:gravity="center_vertical"
	android:background="@color/white"
	android:padding="5dp">
	 <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/SimpleDraweeView_select_brand_logo001"
        android:layout_width="50dp"
        android:layout_height="50dp"
        fresco:actualImageScaleType="focusCrop"
        fresco:roundAsCircle="false" />
	 <TextView
	     android:id="@+id/TextView_select_brand_name001"
	     android:padding="10dp"
	      android:layout_width="wrap_content"
	     android:layout_height="wrap_content"
	     android:text="ongfeng"
	     android:textColor="@color/inputblack"/>
	</LinearLayout>
	<RelativeLayout
	    android:layout_below="@+id/LinearLayout_get001"
	    android:layout_marginTop="1dp"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
	android:orientation="horizontal"
	android:background="@color/transparent">
    <ListView
        android:layout_alignParentLeft="true"
        android:id="@+id/select_seriesinfo_lv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none"/>
    <LinearLayout 
        android:id="@+id/LinearLayout_car_year_style001"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:visibility="gone">
        <TextView
            android:id="@+id/TextView_gone_car_yeay_show001"
             android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:background="@color/transparent"/>
        <View android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/common_bg"/>
         <ListView
             android:layout_weight="2"
        android:layout_alignParentLeft="true"
        android:id="@+id/select_seriesinfo_year_style01"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:scrollbars="none"/>
    </LinearLayout>
    
    </RelativeLayout>
</RelativeLayout>